<nav id="menu" class="nav-menu d-none d-lg-block {% if _context.mobile %}mobile-nav{% endif %} ">
  <ul>
    {% for item in menu %}
      {% if item.menu == "" %}
      {% elseif item.menu == title %}
        <li class="active"><a href="#">{{item.menu}}</a></li>
      {% else %}
        <li><a href="{{item.path}}">{{item.menu}}</a></li>
      {% endif %}
    {% endfor %}
  </ul>
</nav><!-- .nav-menu -->
